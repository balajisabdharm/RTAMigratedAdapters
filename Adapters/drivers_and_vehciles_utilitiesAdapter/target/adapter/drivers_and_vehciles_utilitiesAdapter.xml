<mfp:adapter name="drivers_and_vehciles_utilitiesAdapter" xmlns:http="http://www.ibm.com/mfp/integration/http" xmlns:mfp="http://www.ibm.com/mfp/integration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <displayName>drivers_and_vehciles_utilitiesAdapter</displayName>
    <description>drivers_and_vehciles_utilitiesAdapter</description>
    <connectivity>
        <connectionPolicy xsi:type="http:HTTPConnectionPolicyType">
            <protocol>https</protocol>
            <domain>localhost</domain>
            <port>80</port>
            <connectionTimeoutInMilliseconds>200000</connectionTimeoutInMilliseconds>
            <socketTimeoutInMilliseconds>200000</socketTimeoutInMilliseconds>
            <maxConcurrentConnectionsPerNode>100</maxConcurrentConnectionsPerNode>
        </connectionPolicy>
    </connectivity>
    <procedure name="buildBody" />
    <procedure name="buildBodyFromStaticRequest" />
    <procedure name="getApplicationSettings" />
    <procedure name="getTibcoMode" />
    <procedure name="getChannelCredentials" />
    <procedure name="getExternalChannelCredentials" />
    <procedure name="getTibcoCredentials" />
    <procedure name="sendMail" />
    <procedure name="encryptData" />
    <procedure name="decryptData" />
    <procedure name="deleteCredientails" />
    <procedure name="generatePassword" />
    <procedure name="invokeEncryptedProcedure" />
    <procedure name="getEtrafficDonationServices" />
    <procedure name="getDonationServices" />
    <procedure name="getDonationServices_iOS" />

    <property name="drivers_and_vehicles_is_debugging"  description="drivers_and_vehicles_is_debugging" defaultValue="true"/>
    <property name="wsse.tibco.username"  description="wsse.tibco.username" defaultValue=""/>
    <property name="wsse.tibco.password"  description="wsse.tibco.password" defaultValue=""/>
	<property name="drivers_and_vehicles_mail_port"  description="drivers_and_vehicles_mail_port" defaultValue="465"/>
	<property name="drivers_and_vehicles_mail_host"  description="drivers_and_vehicles_mail_host" defaultValue="smtp.va1.emailserver.com"/>
	<property name="drivers_and_vehicles_mail_toMailAddress"  description="drivers_and_vehicles_mail_toMailAddress" defaultValue="dev.test@intellisc.com"/>
	<property name="drivers_and_vehicles_mail_user"  description="drivers_and_vehicles_mail_user" defaultValue="dev.test@intellisc.com"/>
	<property name="drivers_and_vehicles_mail_pass"  description="drivers_and_vehicles_mail_pass" defaultValue="Password@12"/>
</mfp:adapter>
