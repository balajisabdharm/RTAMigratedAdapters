<mfp:adapter name="drivers_and_vehicles_SalikTagAdapter" xmlns:http="http://www.ibm.com/mfp/integration/http" xmlns:mfp="http://www.ibm.com/mfp/integration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <displayName>drivers_and_vehicles_SalikTagAdapter</displayName>
    <description>drivers_and_vehicles_SalikTagAdapter</description>
    <connectivity>
        <connectionPolicy xsi:type="http:HTTPConnectionPolicyType">
            <protocol>https</protocol>
             <domain>${Eiptibam2rtaglobal}$</domain>
            <port>${Eiptibam2rtaglobalport21020}$</port>
            <connectionTimeoutInMilliseconds>60000</connectionTimeoutInMilliseconds>
            <socketTimeoutInMilliseconds>60000</socketTimeoutInMilliseconds>
            <maxConcurrentConnectionsPerNode>50</maxConcurrentConnectionsPerNode>
        </connectionPolicy>
    </connectivity>
   <procedure name="ValidateTag" />
	<procedure name="ValidateTFNAndPlate" />
	<procedure name="ValidateProfile" />
    <procedure name="VehicleInformationValidation" />
	<procedure name="TagRegistration" />
	<procedure name="ValidateVehicleDetails" />
	<procedure name="TagRegistrationWithProfileDetails" />

    <property name="drivers_and_vehicles_is_debugging"  description="drivers_and_vehicles_is_debugging" defaultValue="${DriversAndVehiclesIsDebugging}$"/>
</mfp:adapter>
